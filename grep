                   "change-background" = readFile ./change-background.sh;
                   "change-dpi" = ''
                     case "$AUTORANDR_CURRENT_PROFILE" in
                       default)
                         DPI=120
                         ;;
                       home)
                         DPI=192
                         ;;
                       work)
                         DPI=144
                         ;;
                       *)
                         echo "Unknown profile: $AUTORANDR_CURRENT_PROFILE"
                         exit 1
                     esac

                     echo "Xft.dpi: $DPI" | ${pkgs.xorg.xrdb}/bin/xrdb -merge
                   ''
                 };
               }

