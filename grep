           _T_y_p_e_: boolean

           _D_e_f_a_u_l_t_: false

           _D_e_c_l_a_r_e_d _b_y_:
               <<hhoommee--mmaannaaggeerr//mmoodduulleess//sseerrvviicceess//wwiinnddooww--mmaannaaggeerrss//xxmmoonnaadd..nniixx>>

       xxsseessssiioonn..wwiinnddoowwMMaannaaggeerr..xxmmoonnaadd..ccoonnffiigg
           The configuration file to be used for xmonad. This must be an absolute path or null in which case ~/.xmonad/xmonad.hs will
           not be managed by Home Manager.

           If this option is set to a non-null value, recompilation of xmonad outside of Home Manager (e.g. via xxmmoonnaadd ----rreeccoommppiillee)
           will fail.

           _T_y_p_e_: null or path

           _D_e_f_a_u_l_t_: null

           _E_x_a_m_p_l_e_:

               pkgs.writeText "xmonad.hs" ''
                 import XMonad
                 main = xmonad defaultConfig
                     { terminal    = "urxvt"
                     , modMask     = mod4Mask
                     , borderWidth = 3
                     }
               ''

           _D_e_c_l_a_r_e_d _b_y_:
               <<hhoommee--mmaannaaggeerr//mmoodduulleess//sseerrvviicceess//wwiinnddooww--mmaannaaggeerrss//xxmmoonnaadd..nniixx>>

       xxsseessssiioonn..wwiinnddoowwMMaannaaggeerr..xxmmoonnaadd..eexxttrraaPPaacckkaaggeess
           Extra packages available to GHC when rebuilding xmonad. The value must be a function which receives the attribute set
           defined in _h_a_s_k_e_l_l_P_a_c_k_a_g_e_s as the sole argument.

           _T_y_p_e_: unspecified value

           _D_e_f_a_u_l_t_: "self: []"

           _E_x_a_m_p_l_e_:

               haskellPackages: [
                 haskellPackages.xmonad-contrib
                 haskellPackages.monad-logger
               ]

           _D_e_c_l_a_r_e_d _b_y_:
               <<hhoommee--mmaannaaggeerr//mmoodduulleess//sseerrvviicceess//wwiinnddooww--mmaannaaggeerrss//xxmmoonnaadd..nniixx>>

